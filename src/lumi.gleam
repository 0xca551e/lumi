pub type Rgb {
  Rgb(r: Float, g: Float, b: Float)
}

pub type Hsl {
  Hsl(h: Float, s: Float, l: Float)
}

pub type Hsv {
  Hsv(h: Float, s: Float, v: Float)
}

pub type Xyz {
  Xyz(x: Float, y: Float, z: Float)
}

pub type Lab {
  Lab(l: Float, a: Float, b: Float)
}

pub type Lchab {
  Lchab(l: Float, c: Float, h: Float)
}

pub type Luv {
  Luv(l: Float, u: Float, v: Float)
}

pub type Lchuv {
  Lchuv(l: Float, c: Float, h: Float)
}

pub type Hsluv {
  Hsluv(h: Float, s: Float, luv: Float)
}
